      "settings": {
        "foreground": "#a9b1d6"
      }
    },
    {
      "name": "Export",
      "scope": ["keyword.control.export", "storage.type"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9d7cd8"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#bb9af7"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
