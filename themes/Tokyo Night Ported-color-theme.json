{
  "name": "Tokyo Night Ported",
  "type": "dark",
  "author": "Dionannd",
  "colors": {
    "editor.background": "#1e2030",
    "editor.foreground": "#c0caf5",
    "activityBarBadge.background": "#9d7cd8",
    "sideBarTitle.foreground": "#a9b1d6",
    "sideBar.background": "#222436",
    "sideBar.foreground": "#a9b1d6",
    "activityBar.background": "#1A1B26",
    "button.background": "#9d7cd8",
    "button.hoverBackground": "#bb9af7",
    "breadcrumb.foreground": "#a9b1d6",
    "breadcrumbPicker.background": "#24283b",
    "titleBar.activeBackground": "#1A1B26",
    "titleBar.activeForeground": "#c0caf5",
    "tab.activeBackground": "#1e2030",
    "tab.inactiveBackground": "#1a1b26",
    "input.background": "#2f334d",
    "input.foreground": "#a9b1d6",
    "input.border": "#292e42"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#565f89"
      }
    },
    {
      "name": "Variables",
      "scope": ["string constant.other.placeholder"],
      "settings": {
        "foreground": "#bb9af7"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#a9b1d6"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff007c"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#7dcfff"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.control.flow.js",
        "keyword.control.flow.tsx",
        "storage.modifier.async.tsx"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f7768e"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": [
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "storage.type.function.arrow",
        "keyword.operator.relational",
        "keyword.operator.increment",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#828bb8"
      }
    },
    {
      "name": "Function",
      "scope": ["storage.type.function", "meta.import"],
      "settings": {
        "fontStyle": "",
        "foreground": "#bb9af7"
      }
    },
    {
      "name": "Definition Variable",
      "scope": ["meta.definition.variable", "variable.other.constant.property"],
      "settings": {
        "fontStyle": "",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Object Literal",
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#a9b1d6"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
